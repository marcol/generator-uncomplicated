{
  "name": "generator-uncomplicated",
  "version": "0.7.4",
  "description": "Uncomplicated Yeoman generator for quick web projects",
  "keywords": [
    "CSS",
    "HTML",
    "basic",
    "fast",
    "javascript",
    "simple",
    "uncomplicated",
    "web",
    "yeoman-generator"
  ],
  "scripts": {
    "lint": "npm run lint:js && npm run lint:md && npm run lint:css",
